<h1>Listing empresas</h1>

<table>
  <tr>
    <th>Razao</th>
    <th>Endereco</th>
    <th>Cep</th>
    <th>Bairro</th>
    <th>Cidade</th>
    <th>Estado</th>
    <th>Cnpj</th>
    <th>Inscricao estadual</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @empresas.each do |empresa| %>
  <tr>
    <td><%= empresa.razao %></td>
    <td><%= empresa.endereco %></td>
    <td><%= empresa.cep %></td>
    <td><%= empresa.bairro %></td>
    <td><%= empresa.cidade %></td>
    <td><%= empresa.estado %></td>
    <td><%= empresa.cnpj %></td>
    <td><%= empresa.inscricao_estadual %></td>
    <td><%= link_to 'Show', empresa %></td>
    <td><%= link_to 'Edit', edit_empresa_path(empresa) %></td>
    <td><%= link_to 'Destroy', empresa, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Empresa', new_empresa_path %>
